{
	"name": "@dillerm/pretty-controls",
	"version": "0.1.1",
	"scripts": {
		"serve": "vue-cli-service serve",
		"build": "vue-cli-service build",
		"lint": "vue-cli-service lint",
		"publish": "npm publish --access public",
		"build-bundle": "vue-cli-service build --target lib --name pretty-controls src/library.js"
	},
	"dependencies": {
		"axios": "^0.18.0",
		"vue": "^2.5.17"
	},
	"devDependencies": {
		"@vue/cli-plugin-babel": "^3.0.4",
		"@vue/cli-plugin-eslint": "^3.0.4",
		"@vue/cli-service": "^3.0.4",
		"node-sass": "^4.9.3",
		"sass-loader": "^7.1.0",
		"style-loader": "^0.23.0",
		"vue-template-compiler": "^2.5.17"
	},
	"main": "./dist/pretty-controls.common.js",
	"files": [
		"dist/*",
		"src/*",
		"public/*",
		"*.json",
		"*.js"
	],
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true
		},
		"extends": [
			"plugin:vue/essential",
			"eslint:recommended"
		],
		"rules": {},
		"parserOptions": {
			"parser": "babel-eslint"
		}
	},
	"postcss": {
		"plugins": {
			"autoprefixer": {}
		}
	},
	"browserslist": [
		"> 1%",
		"last 2 versions",
		"not ie <= 8"
	]
}
